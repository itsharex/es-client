{
    "manifest_version": 2,
    "name": "es-client",
    "version": "0.8.1",
    "description": "elasticsearch管理客户端",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "icons": {
        "16": "icons/logo_16.png"
    },
    "browser_action": {},
    "background": {
        "scripts": [
            "src/background.js"
        ]
    },
    "permissions": [
        "http://*/*",
        "https://*/*"
    ]
}